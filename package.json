{
	"name": "shortcuts-visualizer",
	"description": "TOCONFIGURE",
	"version": "0.0.0-semantically-released",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"babel:run": "npx babel-node --extensions \".ts,.js\" --",
		"babel:run:dev": "npx onchange -i \"src/**/*.ts\" -- npx babel-node --extensions \".ts,.js\" --",
		"build:babel": "npx babel --extensions \".ts,.js\" src --out-dir dist --source-maps inline",
		"build:watch": "npm run build:babel -- --watch",
		"build:clean": "rm -rf dist && npm run build",
		"build:types": "npx tsc -p tsconfig.types.json && npx dts-alias -p tsconfig.types.json",
		"demo:dev": "vite",
		"demo:build": "vue-tsc --noEmit && vite build",
		"demo:serve": "vite preview",
		"build": "npm run build:babel && npm run build:types",
		"lint:eslint": "npx eslint \"{src,tests,src-demo}/**/*.{js,ts,vue}\" \"*.{js,ts,vue}\" --max-warnings=0 --report-unused-disable-directives",
		"lint:types": "npx tsc --noEmit --pretty",
		"lint:commits": "npx commitlint --from $(git rev-list HEAD --not --remotes | tail -1)^ --to HEAD --verbose",
		"lint": "npm run lint:types && npm run lint:eslint",
		"coverage": "npm run test -- --coverage",
		"coverage:dev": "concurrently \"npx http-server coverage/lcov-report --port=5000\" \"onchange -k -i \\\"src/**/*.ts\\\" \\\"jest.config.js\\\" --  npm run coverage -- --watch\"",
		"test": "npm run lint:types && npx jest test --verbose",
		"test:debug": "node --inspect-brk node_modules/jest/bin/jest test --verbose",
		"test:inspect-errors": "set INSPECT_ERRORS=true && npx test",
		"doc": "npx typedoc --options typedoc.config.js",
		"doc:build": "npx onchange -i \"src/**/*.ts\" \"typedoc.config.js\" -- npm run doc",
		"doc:serve": "npx http-server docs --port=5001",
		"doc:dev": "npx concurrently \"yarn doc:build\" \"yarn doc:serve\"",
		"doc:check-invalid": "npx typedoc --options typedoc.config.js --listInvalidSymbolLinks",
		"doc:check-missing": "rg \"@packageDocumentation @module\" --files-without-match -g \"*.ts\" -g \"!*.spec.ts\" -g \"!tests/**/*\" -g \"!src/index.ts\"",
		"actions:debug": "act -r -v -j build-only",
		"gen:exports": "npx indexit update",
		"audit:prod": "yarn audit --level moderate --groups dependencies",
		"prepare": "npm run build && husky install"
	},
	"dependencies": {
		"@alanscodelog/utils": "^0.0.0-semantically-released",
		"core-js": "^3.19.1",
		"lodash": "4.17.21"
	},
	"devDependencies": {
		"@alanscodelog/babel-preset": "^2.0.0",
		"@alanscodelog/commitlint-config": "^2.0.0",
		"@alanscodelog/eslint-config": "^3.0.0",
		"@alanscodelog/semantic-release-config": "^2.0.0",
		"@alanscodelog/tsconfigs": "^3.0.0",
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.0",
		"@babel/node": "^7.16.0",
		"@babel/plugin-proposal-class-properties": "^7.16.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"@babel/preset-typescript": "^7.16.0",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/commit-analyzer": "^9.0.1",
		"@semantic-release/github": "^8.0.2",
		"@semantic-release/npm": "^8.0.3",
		"@semantic-release/release-notes-generator": "^10.0.2",
		"@types/chai": "^4.2.22",
		"@types/jest": "^27.0.2",
		"@types/lodash": "4.14.176",
		"@types/node": "^16.11.7",
		"@types/spotify-web-api-node": "^5.0.3",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"@vitejs/plugin-vue": "^1.9.4",
		"@vue/compiler-sfc": "^3.2.21",
		"@vue/eslint-config-typescript": "^9.0.1",
		"babel-plugin-module-resolver": "^4.1.0",
		"chai": "^4.3.4",
		"commitlint": "^14.1.0",
		"concurrently": "^6.4.0",
		"conventional-changelog-conventionalcommits": "^4.6.1",
		"dts-alias": "^1.0.5",
		"eslint": "^8.2.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsdoc": "^37.0.3",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-vue": "^8.0.3",
		"husky": "^7.0.0",
		"indexit": "^1.3.0",
		"jest": "^27.3.1",
		"jest-watch-typeahead": "^1.0.0",
		"onchange": "^7.1.0",
		"sass": "^1.43.4",
		"semantic-release": "^18.0.0",
		"type-fest": "^2.5.3",
		"typedoc": "^0.22.8",
		"typedoc-neo-theme": "^1.1.1",
		"typedoc-plugin-param-names": "^3.1.0",
		"typescript": "^4.4.4",
		"vite": "^2.6.14",
		"vue": "^2.6.14",
		"vue-tsc": "^0.29.4"
	},
	"author": "Alan <alanscodelog@gmail.com>",
	"repository": "https://github.com/alanscodelog/shortcuts-visualizer",
	"license": "MIT",
	"files": [
		"src",
		"dist"
	],
	"release": {
		"extends": [
			"@alanscodelog/semantic-release-config"
		]
	},
	"commitlint": {
		"extends": [
			"@alanscodelog"
		]
	},
	"babel": {
		"presets": [
			"@alanscodelog"
		]
	},
	"browserslist": "> 0.5%, last 2 versions, not dead, not < 0.25%, not IE > 0, maintained node versions",
	"engines": {
		"node": ">=14.0.0"
	},
	"@comments": {
		"scripts": {
			"test": "Runs `lint:types` before (so that flags can be passed to the test command) so that we can test type assertions. See expect_type function in @alanscodelog/utils.",
			"lint:commits": "Lints all unpushed commits in the active branch.",
			"test:inspectErrors": "For use with my inspectError utility function from @alanscodelog/utils",
			"prepare": "Needed so that if we pull the package from git it will get built and installed properly.",
			"actions:debug": "For debugging github build action locally with nektos/act. Requires act and docker. Note: Cache will never work locally because of https://github.com/nektos/act/issues/285",
			"demo:lint": "Linting is hit or miss on VS Code."
		}
	},
	"private": true,
	"TEMPLATE:bca349b": "Ignore. Commit of the template this was created with / last updated to."
}
