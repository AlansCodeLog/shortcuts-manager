{
	"name": "shortcuts-manager-demo",
	"private": "true",
	"type": "module",
	"scripts": {
		"debug": "ts-node -r tsconfig-paths/register -T --esm",
		"dev": "vite",
		"build": "vite build",
		"build:dev": "vite build --mode development",
		"build:watch": "vite build --watch --mode production",
		"build:types": "vue-tsc -p tsconfig.types.json && npm run build:types:fix",
		"build:types:fix": "tsc-alias -p tsconfig.types.json --debug",
		"lint:imports": "madge --circular --extensions ts ./src",
		"lint:types": "vue-tsc --noEmit --pretty",
		"prepare": "pnpm link ../ && npm run build"
	},
	"dependencies": {
		"@alanscodelog/utils": "4.0.0-beta.10",
		"@alanscodelog/vue-components": "^1.3.4",
		"@fortawesome/fontawesome-svg-core": "^6.4.0",
		"@fortawesome/free-regular-svg-icons": "^6.4.0",
		"@fortawesome/free-solid-svg-icons": "^6.4.0",
		"metamorphosis": "^0.2.3"
	},
	"devDependencies": {
		"@types/node": "^20.4.3",
		"@vitejs/plugin-vue": "^4.2.3",
		"autoprefixer": "^10.4.14",
		"madge": "^6.1.0",
		"postcss": "^8.4.27",
		"postcss-comment": "^2.0.0",
		"postcss-import": "^15.1.0",
		"shortcuts-manager": "file:../",
		"tailwindcss": "^3.3.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.6",
		"vite-plugin-dts": "^3.3.1",
		"vite-plugin-externalize-deps": "^0.7.0",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.33.0",
		"vue": "^3.3.4",
		"vue-tsc": "^1.8.6"
	},
	"author": "Alan <alanscodelog@gmail.com>",
	"repository": "https://github.com/alanscodelog/shortcuts-visualizer"
}
